<GuiXml>
    <Controls>
        <TopLevelControl 	name="AutoK" 
							mouseEnabled="true" 
							clampedToScreen="true" 
							movable="true">
            <Dimensions x="200" y="200" />
            <Anchor point="TOPLEFT" offsetX="1400" offsetY="50"/>
 
 
            <OnUpdate>
                AutoKUpdate()
            </OnUpdate>
 
            <Controls>
                <Label 	name="$(parent)Time" 
						font="ZoFontGame" color="CFDCBD" 
						relativeTo="$(parent)" relativePoint="BOTTOMRIGHT"
						text="">
						<Anchor point="TOPLEFT" offsetX="4" offsetY="4"/>
                </Label>
				
				<Label 	name="$(parent)PartySize" 
						font="ZoFontGame" color="CFDCBD" 
						text="">
						<Anchor relativeTo="$(parent)Time" relativePoint="BOTTOMLEFT"
								point="TOPLEFT" offsetX="0" offsetY="4"/>
                </Label>
				
				<Label 	name="$(parent)Active" 
						font="ZoFontGame" color="CFDCBD" 
						text="">
						<Anchor relativeTo="$(parent)PartySize" relativePoint="BOTTOMLEFT"
								point="TOPLEFT" offsetX="0" offsetY="4"/>
                </Label>
				
				<Label 	name="$(parent)CampaignInfo" 
						font="ZoFontGame" color="CFDCBD" 
						text="">
						<Anchor relativeTo="$(parent)Active" relativePoint="BOTTOMLEFT"
								point="TOPLEFT" offsetX="0" offsetY="4"/>
                </Label>
				
				<Label 	name="$(parent)Members" 
						font="ZoFontGame" color="CFDCBD" 
						text="">
						<Anchor relativeTo="$(parent)CampaignInfo" relativePoint="BOTTOMLEFT"
								point="TOPLEFT" offsetX="0" offsetY="4"/>
                </Label>
            </Controls>

        </TopLevelControl>
    </Controls>
</GuiXml>